
# aliases
alias dev="cd $HOME/Developer/"
alias rl="exec zsh -l"
alias cl="clear"
alias ne="node"
alias nm="nvm"
alias bw="brew"
alias pm="pnpm"
alias th="trash"
alias md="mkdir -p"
alias ls="ls -G"
alias lf="ls -AFG"
alias ll="ls -al -G"
alias la="ls -a -G"
alias l1="ls -1 -G"
alias -- -="cd -"

# scripts aliases
alias bu="$HOME/.config/scripts/brew.sh"
alias up="$HOME/.config/scripts/upgrade.sh"
alias ck="$HOME/.config/scripts/comics.sh"
alias dk="$HOME/.config/scripts/data.sh"
